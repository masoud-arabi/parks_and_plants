 <div class="container">
 <h1> list all gardens </h1>
 <ul>
  <% @gardens.each do |garden| %>
 <li><%= link_to garden.name, garden_path(garden) %>
 <%= link_to 'delete', garden_path(garden),method: "delete", data: {confirm: 'Are you sure'} %></li>
 <% end %>
 </ul>
 <%= link_to 'create new garden', new_garden_path %></li>
 </div>